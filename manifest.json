{
    "name": "Meloddi Extension",
    "description": "An extension that helps users add products to their registry",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
	"service_worker": "background.js"
    },
    "action": {
	"default_popup": "popup.html",
	"default_icon": {
	    "16": "/assets/logo16.png",
	    "32": "/assets/logo32.png",
	    "48": "/assets/logo48.png",
	    "128": "/assets/logo128.png"
	}
	
    },
    "icons": {
	"16": "/assets/logo16.png",
         "32": "/assets/logo32.png",
         "48": "/assets/logo48.png",
        "128": "/assets/logo128.png"
    }
}
